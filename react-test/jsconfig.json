{
  "compilerOptions": {
    "target": "ES6",
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "modules/*": ["src/modules/*"],
      "components/*": ["src/components/*"],
      "constants/*": ["src/constants/*"],
      "styles/*": ["src/styles/*"],
      "utils/*": ["src/utils/*"],
      "selectors/*": ["src/selectors/*"],
      "store/*": ["src/store/*"],
      "assets/*": ["src/assets/*"],
      "http/*": ["src/http/*"],
      "actions/*": ["src/redux/actions/*"],
      "indexJS/*": ["src/indexJS/*"],
      "image/*": ["src/image/*"]
    }
  },
  "exclude": [
    "node_modules",
    "dist",
    "server"
  ]
}